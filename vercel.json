{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(assets/.*)",
            "dest": "/build/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/build/index.html"
        }
    ]
}